#include <stdio.h>
#include<string.h>
int main() {
        int i,n;
        FILE * fptr;
        char str[10];
       printf("enter data");
     gets(str);
n=strlen(str);
        fptr = fopen("test.doc", "w");
        for (i = 0;i<n; i++) 
        {
            fputc(str[i], fptr);
        }
     printf("File created");
        fclose(fptr);
        return 0;
    }
