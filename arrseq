#include <stdio.h>
      void main()
      {
        int i, j,n,a[30],t;
        printf("Enter the value of N \n");
        scanf("%d", &n);
        printf("Enter the numbers \n");
        for (i = 0; i < n; ++i)
         {
            scanf("%d", &a[i]);
          }
        for (i = 0; i < n; ++i) 
        {
             for (j = i + 1; j < n; ++j)
            {
                 if (a[i] > a[j]) 
                {
                     t=a[i]; 
                    a[i]=  a[j];
                    a[j] = t;
                  }
             }
         }
 
        printf("The numbers  in ascending order are \n");
        for (i = 0; i < n; ++i)
        printf("%d\n", a[i]);
        reutn 0;
     }
